{
  "name": "giji-gatsby",
  "private": true,
  "description": "人狼議事",
  "author": "7korobi@gmail.com",
  "version": "0.1.0",
  "dependencies": {
    "date-fns": "^2.14.0",
    "dexie": "^3.0.1",
    "fancy-date": "^0.3.12",
    "firebase": "^7.15.5",
    "firebase-admin": "^8.13.0",
    "firebase-functions": "^3.7.0",
    "gatsby": "^2.23.14",
    "gatsby-image": "^2.4.11",
    "gatsby-plugin-manifest": "^2.4.16",
    "gatsby-plugin-offline": "^3.2.15",
    "gatsby-plugin-react-helmet": "^3.3.8",
    "gatsby-plugin-sharp": "^2.6.16",
    "gatsby-plugin-typescript": "^2.4.10",
    "gatsby-source-apiserver": "^2.1.6",
    "gatsby-source-filesystem": "^2.3.16",
    "gatsby-transformer-sharp": "^2.5.9",
    "lodash": "^4.17.15",
    "memory-orm": "^0.4.22",
    "parcel": "^1.12.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-firebase-hooks": "^2.2.0",
    "react-helmet": "^6.1.0",
    "react-petit-hooks": "^0.2.5",
    "slate": "^0.58.3",
    "slate-history": "^0.58.3",
    "slate-hyperscript": "^0.58.3",
    "slate-react": "^0.58.3",
    "tiny-cookie": "^2.3.2"
  },
  "devDependencies": {
    "decaffeinate": "^6.0.9",
    "jest": "^26.1.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^1.7.2",
    "yaml-jest": "^1.0.5",
    "yaml-loader": "^0.6.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "dev": "gatsby develop",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "jest": "jest -u --config src/config/jest.js --rootDir .",
    "test": "run-s jest",
    "func": "run-s copy:func deploy:func",
    "precommit": "yarn",
    "copy": "run-s copy_all dist:copy",
    "copy_all": "run-p -n copy:*",
    "copy:sass": "ls src/styl/*.use.sass | xargs -L 1 node-sass --precision 1 --output-style compressed -o static/css/",
    "copy:portrate": "rsync -avz ../../*/web_work/images/portrate/* static/images/portrate/.",
    "copy:func": "parcel build src/api/index.ts --target=node -d functions -o index.js --no-minify",
    "dist:brotli": "find dist -type f -not -name \"*.br\" | grep -v \"public/font\" | xargs -L1  brotli -Z -f -k",
    "deploy": "run-s build deploy:img backup",
    "deploy:test": "run-p -n deploy:host deploy:aws",
    "deploy:live": "run-s -n dist:brotli deploy:rsync deploy:aws deploy:aws_img backup",
    "deploy:sass": "run-s copy:sass dist:copy deploy:test",
    "deploy:sow": "run-s copy:sow dist:copy deploy:test",
    "deploy:img": "run-s copy:portrate dist:copy deploy:test",
    "deploy:copy": "run-s copy dist:copy",
    "deploy:aws_img": "aws s3 sync public/images s3://giji-assets/images --acl public-read --exclude '*.html'",
    "deploy:aws": "aws s3 sync dist s3://giji-assets/fire --acl public-read --exclude '*.html'",
    "deploy:rsync": "rsync -avz -e 'ssh -i ../.ssh/id_rsa' dist 7korobi@giji.f5.si:/home/7korobi/giji-fire/.",
    "deploy:all": "firebase deploy",
    "deploy:host": "firebase deploy --only hosting",
    "deploy:func": "firebase deploy --only functions",
    "g": "run-p -n g:*",
    "g:sudo": "npm install -g windows-build-tools",
    "g:msys": "pacman -S openssh rsync",
    "g:npm": "npm install -g aws-sdk firebase-tools npm-check-updates typesync"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
